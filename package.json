{
  "name": "nextjs-static-export-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run '/^(dev|watch):.*/'",
    "dev:next": "next dev -p 3000",
    "dev:storybook": "storybook dev -p 6006",
    "build": "pnpm run '/^build:.*$/'",
    "build:next": "rm -rf .next/cache/fetch-cache && next build",
    "build:storybook": "storybook build",
    "chromatic": "chromatic --build-script-name='build:storybook'",
    "start": "pnpm run '/^start:.*$/'",
    "start:next": "serve out -p 8000",
    "start:storybook": "serve storybook-static -p 6008",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "test:unit": "vitest --project=unit",
    "test:unit-coverage": "vitest run --project=unit --coverage",
    "test:unit-ui": "vitest --project=unit --reporter='html' --outputFile='.vitest/ui/index.html' --ui",
    "test:storybook": "vitest --project=storybook",
    "test:storybook-coverage": "vitest run --project=storybook --coverage",
    "eslint:check": "next lint",
    "eslint:fix": "next lint --fix",
    "markuplint:check": "markuplint 'src/**/*.{jsx,tsx,html}' -p",
    "markuplint:fix": "pnpm markuplint:check --fix",
    "lint:check": "pnpm eslint:check && pnpm markuplint:check",
    "lint:fix": "pnpm eslint:fix && pnpm markuplint:fix",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css}' --cache",
    "format:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}' --cache",
    "typecheck": "tsc --noEmit",
    "spellcheck": "cspell --no-progress 'src/**/*.{js,jsx,ts,tsx,css}'",
    "quality:check": "pnpm lint:check && pnpm format:check && pnpm typecheck && pnpm spellcheck",
    "quality:fix": "pnpm lint:fix && pnpm format:fix && pnpm typecheck && pnpm spellcheck",
    "lefthook": "lefthook",
    "prepare": "lefthook install",
    "imageIndex": "pnpm ts-node scripts/genImageIndex.ts",
    "watch:imageIndex": "chokidar 'src/images/**/*.{png,jpg,jpeg,gif,svg,webp}' -c 'pnpm imageIndex'",
    "ts-node": "node --experimental-strip-types --experimental-transform-types --experimental-detect-module --no-warnings=ExperimentalWarning",
    "generate": "plop",
    "generate:component": "plop component",
    "generate:page": "plop page",
    "generate:layout": "plop layout"
  },
  "dependencies": {
    "@next/third-parties": "15.3.3",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "dompurify": "3.2.6",
    "html-react-parser": "5.2.5",
    "jsdom": "26.1.0",
    "microcms-js-sdk": "3.2.0",
    "next": "15.3.3",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "schema-dts": "1.1.5",
    "server-only": "0.0.1",
    "tailwind-merge": "3.3.0",
    "tailwind-variants": "1.0.0",
    "ts-pattern": "5.7.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.0.0",
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@markuplint/jsx-parser": "4.7.19",
    "@markuplint/ml-config": "4.8.11",
    "@markuplint/react-spec": "4.5.19",
    "@mdx-js/react": "3.1.0",
    "@next/eslint-plugin-next": "15.3.3",
    "@storybook/addon-a11y": "9.0.6",
    "@storybook/addon-docs": "9.0.6",
    "@storybook/addon-links": "9.0.6",
    "@storybook/addon-vitest": "9.0.6",
    "@storybook/nextjs-vite": "9.0.6",
    "@tailwindcss/postcss": "4.1.8",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.30",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.1",
    "@vitest/browser": "3.2.3",
    "@vitest/coverage-v8": "3.2.3",
    "@vitest/eslint-plugin": "1.2.1",
    "@vitest/ui": "3.2.3",
    "chokidar-cli": "3.0.0",
    "chromatic": "12.2.0",
    "cspell": "9.0.2",
    "eslint": "9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.0.6",
    "eslint-plugin-unused-imports": "4.1.4",
    "jiti": "2.4.2",
    "lefthook": "1.11.13",
    "markuplint": "4.12.0",
    "msw": "2.10.2",
    "msw-storybook-addon": "2.0.5",
    "playwright": "1.52.0",
    "plop": "4.0.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.12",
    "serve": "14.2.4",
    "storybook": "9.0.6",
    "styled-jsx": "5.1.7",
    "tailwindcss": "4.1.8",
    "typescript": "5.8.3",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "22",
    "pnpm": "10"
  },
  "packageManager": "pnpm@10.11.1",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
